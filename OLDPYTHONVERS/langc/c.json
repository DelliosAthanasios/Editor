{
  "rules": [
    {
      "patterns": ["\\b(auto|break|case|char|const|continue|default|do|double|else|enum|extern|float|for|goto|if|inline|int|long|register|restrict|return|short|signed|sizeof|static|struct|switch|typedef|union|unsigned|void|volatile|while|_Alignas|_Alignof|_Atomic|_Bool|_Complex|_Generic|_Imaginary|_Noreturn|_Static_assert|_Thread_local)\\b"],
      "format": {"color": "#569CD6", "bold": true}
    },
    {
      "patterns": ["//.*"],
      "format": {"color": "#6A9955"}
    },
    {
      "patterns": ["/\\*.*\\*/"],
      "format": {"color": "#6A9955"}
    },
    {
      "patterns": ["\"[^\"]*\"", "'[^']*'"],
      "format": {"color": "#CE9178"}
    }
  ],
  "multiline": [
    {
      "start": "/\\*",
      "end": "\\*/",
      "format": {"color": "#6A9955"}
    }
  ]
}