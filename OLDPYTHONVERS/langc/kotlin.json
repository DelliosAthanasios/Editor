{
  "rules": [
    {
      "patterns": ["\\b(as|break|class|continue|do|else|false|for|fun|if|in|interface|is|null|object|package|return|super|this|throw|true|try|typealias|val|var|when|while|by|catch|constructor|delegate|dynamic|field|file|finally|get|import|init|param|property|receiver|set|setparam|where)\\b"],
      "format": {"color": "#569CD6", "bold": true}
    },
    {
      "patterns": ["//.*"],
      "format": {"color": "#6A9955"}
    },
    {
      "patterns": ["/\\*.*\\*/"],
      "format": {"color": "#6A9955"}
    },
    {
      "patterns": ["\"[^\"]*\"", "'[^']*'"],
      "format": {"color": "#CE9178"}
    }
  ],
  "multiline": [
    {
      "start": "/\\*",
      "end": "\\*/",
      "format": {"color": "#6A9955"}
    }
  ]
}