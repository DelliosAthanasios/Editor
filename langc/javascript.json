{
  "rules": [
    {
      "patterns": ["\\b(break|case|catch|class|const|continue|debugger|default|delete|do|else|export|extends|finally|for|function|if|import|in|instanceof|let|new|return|super|switch|this|throw|try|typeof|var|void|while|with|yield)\\b"],
      "format": {"color": "#569CD6", "bold": true}
    },
    {
      "patterns": ["//.*"],
      "format": {"color": "#6A9955"}
    },
    {
      "patterns": ["/\\*.*\\*/"],
      "format": {"color": "#6A9955"}
    },
    {
      "patterns": ["\"[^\"]*\"", "'[^']*'", "`[^`]*`"],
      "format": {"color": "#CE9178"}
    }
  ],
  "multiline": [
    {
      "start": "/\\*",
      "end": "\\*/",
      "format": {"color": "#6A9955"}
    }
  ]
}